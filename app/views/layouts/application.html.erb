<!DOCTYPE html>
<html>
  <head>
    <title>Imam Sidyot</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= favicon_link_tag asset_path('salam_logo.png') %>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag 'all.min' %>
    <%= stylesheet_link_tag 'aos' %>
    <%= stylesheet_link_tag 'responsive' %>
    <%= stylesheet_link_tag 'swiper' %>
    <%= stylesheet_link_tag  'main' %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <%= javascript_importmap_tags %>
    <%= tinymce_assets %>
    <script type="text/javascript" src="/assets/tinymce.js"></script>
    <script src="https://cdn.tiny.cloud/1/no-api-key" referrerpolicy="origin"></script>
